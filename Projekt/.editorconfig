# elemente websolutions

## top-most EditorConfig file
root = true

## Standard Konfiguration
[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
indent_style = tab
indent_size = 4
tab_width = 4

